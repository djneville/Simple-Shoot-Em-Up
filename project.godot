; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SimpleShootEmUp"
run/main_scene="res://godot/Menus/MainMenu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=30
config/icon="res://icon.svg"

[autoload]

GameStatsManager="*res://godot/Autoloads/GameStatsManager.gd"
UpgradeManager="*res://godot/Autoloads/UpgradeManager.gd"
SceneManager="*res://godot/Autoloads/SceneManager.gd"
SignalBus="*res://godot/Autoloads/SignalBus.gd"
ProjectileFactory="*res://godot/Autoloads/ProjectileFactory.gd"
PathManager="*res://godot/Autoloads/PathManager.gd"
ItemManager="*res://godot/Autoloads/ItemManager.gd"
AudioManager="*res://godot/Autoloads/AudioManager.gd"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1
gdscript/warnings/return_value_discarded=1

[display]

window/size/viewport_width=540
window/size/viewport_height=960
window/size/initial_position_type=0
window/dpi/allow_hidpi=false

[editor]

naming/scene_name_casing=1
naming/script_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/script-tabs/plugin.cfg")

[gui]

theme/custom="res://Resources/Fonts/maintheme.tres"

